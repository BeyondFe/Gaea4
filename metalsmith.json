{
    "**/**":[{
        "find":"\"name\": \"gaea\"",
        "replace":"\"name\": \"{{projectName}}\""
    },{
        "find":"\"version\": \"1.0.0\"",
        "replace":"\"version\": \"{{projectVersion}}\""
    },{
        "find":"\"description\": \"description\"",
        "replace":"\"description\": \"{{projectDescription}}\""
        
    },{
        "find":"\"author\": \"author\"",
        "replace":"\"author\": \"{{author}}\""
    },{
        "find":"/测试服务器地址/g",
        "replace":"{{uploadHost}}"
    },{
        "find":"\"carefree\": \"cross-env NODE_ENV=carefree carefree_env=dev webpack  --mode development --env.carefree -w --colors --progress\"",
        "replace":"{{#Carefree}}\n\"carefree\": \"cross-env NODE_ENV=carefree carefree_env=dev webpack  --mode development --env.carefree -w --colors --progress\"\n{{/Carefree}}"
    },{
        "find":"\"dll\": \"webpack --config ./webpack.dll.config.js --mode production --hide-modules --progress\",",
        "replace":"\"dll\": \"webpack --config ./webpack.dll.config.js --mode production --hide-modules --progress\"{{#Carefree}},{{/Carefree}}"
    },{
        "find":"\"@nutui/carefree\": \"^0.2.0\",",
        "replace":"{{#Carefree}}\"@nutui/carefree\": \"{{carefreeVersion}}\",{{/Carefree}}"
    },{
        "find":"const Carefree             = require('@nutui/carefree');",
        "replace":"{{#Carefree}}const Carefree             = require('@nutui/carefree');{{/Carefree}}"
    },{
        "find":"const Smock                = require('smock-webpack-plugin');",
        "replace":"{{#Smock}}const Smock                = require('smock-webpack-plugin');{{/Smock}}"
    },{
        "find":"\"smock-webpack-plugin\": \"^1.0.1\",",
        "replace":"{{#Smock}}\"smock-webpack-plugin\": \"^{{SmockVersion}}\",{{/Smock}}"
    },{
        "find":"new Smock(config.smock)",
        "replace":"{{#Smock}}new Smock(config.smock){{/Smock}}"
    }]
}